FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight   12;
    }

    transport
    {
        kappa   13.0;        // Thermal Conductivity    -   W/m.K
    }

    thermodynamics
    {
        Hf      0;           // Heat of Formation        - J/kg
        Cp      440;         // Heat capacity            - J/kg.K
    }

    equationOfState
    {
        rho     8140;        // Density                  - kg/m3
    }
}

